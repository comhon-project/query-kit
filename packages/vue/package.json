{
  "name": "@query-kit/vue",
  "version": "0.11.0",
  "description": "Vue 3 library for building complex queries and displaying object collections with an intuitive user interface",
  "author": "Jean-Philippe Perrotton <jeanphilippe.perrotton@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "source": "./index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "vue",
    "vue3",
    "query",
    "filter",
    "search",
    "collection",
    "query-builder",
    "data-table",
    "filtering",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/comhon-project/query-kit.git",
    "directory": "packages/vue"
  },
  "homepage": "https://github.com/comhon-project/query-kit",
  "bugs": {
    "url": "https://github.com/comhon-project/query-kit/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "luxon": "^3.7.2",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@eslint/js": "^9.17.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@query-kit/themes": "*",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.2.4",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.7.4",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
